 <nav class="navbar navbar-light">
    <a class="navbar-brand" href="#">
      <%= link_to authenticated_root_path, method: :get do %>
        <%= image_tag('backarrow.png', class: 'backarrow') %>
      <% end %>
      <div class="text-center">
        <h2>LOGIN</h2>
      </div>
      <div class='log'>
      <p class='p'>Log In</p>
      </div>
    </a>
  </nav>
<div class= "container">
  <div class= "child mt-2">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class='full-width'>
        <div class="field mb-3">
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class:'pr mobile text-input form-control', placeholder:'Email' %>
        </div>
        <br/>
        <div class="field mb-3">
          <%= f.password_field :password, autocomplete: "current-password", class:'pr mobile text-input form-control', placeholder:'Password' %>
        </div>
        <br/>
      </div>
      <div class="actions">
        <%= f.submit "Log in", class:'btn btn-primary' %>
      </div>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>
